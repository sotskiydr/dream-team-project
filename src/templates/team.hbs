{{#each this}}
  <li class='team__item'>
    <a class='hero__link' href='{{linkedin}}'>
      <img class='hero__image' src='{{photo}}' alt='{{name}}' />
    </a>
    <div class='hero__info'>
      <p class='hero__name'>{{name}}</p>
      <p class='hero__position'>{{position}}</p>
      <p class='hero__city'>{{city}}</p>
    </div>
  </li>
{{/each}}